{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "ui":{
      "path":"src/App/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates":{
        "component":"grc_templates/ui/TemplateName.tsx"
      }
    },
    "layout":{
      "path":"src/App/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates":{
        "component":"grc_templates/layout/TemplateName.tsx",
        "test":"grc_templates/layout/TemplateName.test.tsx"
      }
    },
      "connectedCmp":{
        "path":"src/App/components",
        "withStyle": true,
        "withTest": true,
        "withStory": false,
        "withLazy": false,
        "customTemplates":{
          "component":"grc_templates/connectedFunctionComponent/TemplateName.tsx"
        } 
      },
      "store":{
        "path":"src/App/store",
        "withStyle": false,
        "withTest": false,
        "withStory": false,
        "withLazy": false,
        "customTemplates":{
          "component":"grc_templates/store/TemplateName.js"
        } 
      }
  }
}